{
	"manifest_version": 2,
	"name": "Monitor Plugin",
	"version": "1.3",
	"description": "Logging what page the user is on",
	"icons": {"128": "img/icon_128.png"},
	"background": {
		"scripts": ["js/jquery-3.3.1.min.js", "js/background.js", "js/saveTextAreaToFile.js"],
		"persistent": false
	},
	"browser_action": {
		"default_icon": "img/icon.png",
		"default_popup": "html/popup.html"
	},
	"permissions": [
		"tabs", "storage"
	]
}